{
    "project_name": "Project_name",
    "project_short_description": "A short description of the project",
    "project_slug": "{{ cookiecutter.project_name.lower().replace('cookiecutter', '').strip().replace(' ', '_') | regex_replace('\\W+', '') }}",
    "package_dist_dir_type": [
        "Relative",
        "Absolute"
    ],
    "package_dist_dir": "../../package/dist",
    "copy_from_package_dist": [
        "YES",
        "NO"
    ],
    "desktop_entrypoint": "run_desktop.py",
    "desktop_icon_icns": "./dist/app/static/images/icon.icns",
    "desktop_noconsole": [
        "YES",
        "NO"
    ],
    "desktop_windowed": [
        "YES",
        "NO"
    ],
    "desktop_noconfirm": [
        "YES",
        "NO"
    ],
    "desktop_clean": [
        "YES",
        "NO"
    ],
    "desktop_add_data_points": {
        "add_data": [
            "app;app"
        ]
    },
    "desktop_hidden_imports": {
        "hidden_import": [
            "engineio.async_drivers"
        ]
    },
    "desktop_copy_files_and_folders_to_local_foler": {
        "files_or_folders": [
            "dist/config.py",
            "dist/databases"
        ]
    },
    "_copy_without_render": [],
    "_extensions": [
        "jinja2_ansible_filters.AnsibleCoreFiltersExtension"
    ]
}